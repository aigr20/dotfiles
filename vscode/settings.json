{
    "workbench.startupEditor": "none",
    "editor.fontFamily": "'JetBrains Mono',  'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "window.menuBarVisibility": "compact",
    "window.restoreWindows": "none",
    "python.pythonPath": "/usr/bin/python3",
    "editor.fontSize": 18,
    "editor.linkedEditing": true,
    "files.associations": {
        "*.cgi": "python"
    },
    "python.linting.pylintArgs": [
        "--rcfile=/home/wilhelm/.config/.pylintrc"
    ],
    "python.autoComplete.addBrackets": true,
    "python.analysis.completeFunctionParens": true,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "eslint.options": {
        "parserOptions": {
            "ecmaVersion": 2018
        },
        "configFile": "/home/wilhelm/.config/.eslintrc.json",
        "resolvePluginsRelativeTo": "/usr/local/lib"
    },
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.formatOnSave": true,
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "workbench.enableExperiments": false,
    "python.languageServer": "Pylance",
    "python.formatting.autopep8Args": [
        "--max-line-length=100"
    ],
    "terminal.explorerKind": "external",
    "workbench.colorTheme": "Ros√© Pine Dawn",
    "python.showStartPage": false,
    "js/ts.implicitProjectConfig.checkJs": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        ".",
        "-p",
        "*test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.unittestEnabled": true,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "python.defaultInterpreterPath": "python3",
    "eslint.format.enable": true,
    "editor.mouseWheelZoom": true,
    "editor.parameterHints.cycle": true,
    "files.eol": "\n",
    "workbench.iconTheme": "rose-pine-icons",
    "workbench.editor.closeOnFileDelete": true,
    "eslint.nodePath": "/usr/local/lib/node_modules/",
    "javascript.validate.enable": false,
    "php.suggest.basic": false,
    "intelephense.environment.phpVersion": "7.4.16",
    "intelephense.format.enable": false,
    "gitlens.defaultDateFormat": null,
    "gitlens.defaultTimeFormat": "HH:mm",
    "editor.multiCursorModifier": "ctrlCmd",
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "colorize.languages": [
        "javascript",
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg"
    ],
    "editor.minimap.enabled": false
}